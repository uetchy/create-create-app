{
  "name": "create-create-app",
  "description": "Create your own `create-something` app.",
  "version": "5.6.1",
  "author": "Yasuaki Uechi <y@uechi.io>",
  "scripts": {
    "build": "shx rm -rf lib && yarn build:ts --minify --dts",
    "build:ts": "tsup src/index.ts src/cli.ts -d lib",
    "dev": "yarn build:ts --watch",
    "prepublishOnly": "yarn build",
    "test": "echo notest"
  },
  "dependencies": {
    "@types/inquirer": "^7.3.0",
    "@types/yargs-interactive": "^2.1.1",
    "chalk": "^4.1.0",
    "cross-spawn": "^7.0.3",
    "epicfail": "^0.4.2",
    "execa": "^4.0.3",
    "gitconfig": "^2.0.8",
    "globby": "^11.0.1",
    "handlebars": "^4.7.6",
    "inquirer": "^7.3.2",
    "is-utf8": "^0.2.1",
    "license.js": "^3.1.2",
    "slash": "^3.0.0",
    "uuid": "^8.2.0",
    "yargs-interactive": "^3.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^14.0.24",
    "@types/uuid": "^8.0.0",
    "doctoc": "^1.4.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "shx": "^0.3.2",
    "ts-node": "^8.10.2",
    "tsup": "^3.1.1",
    "typescript": "^3.9.7"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "files": [
    "lib",
